{
	"name": "sveltekit-template",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"database:generate-migrations": "drizzle-kit generate:pg"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@types/cookie": "^0.5.1",
		"autoprefixer": "^10.4.15",
		"drizzle-kit": "^0.19.13",
		"nodemailer": "^6.9.5",
		"postcss": "^8.4.29",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "0.14.0",
		"@auth/drizzle-adapter": "0.3.2",
		"@auth/sveltekit": "0.3.7",
		"clsx": "^2.0.0",
		"drizzle-orm": "^0.28.6",
		"postgres": "^3.3.5",
		"tailwind-merge": "^1.14.0"
	},
	"pnpm": {
		"overrides": {
			"@auth/core": "0.14.0"
		}
	}
}